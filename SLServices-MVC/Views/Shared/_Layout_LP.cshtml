<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - Two Canada Experience</title>

	<!-- Favicons -->
	<link href="~/lp/assets/img/favicon.png" rel="icon">
	<link href="~/lp/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

	<!-- Google Fonts -->
	<link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Raleway:300,400,500,700,800" rel="stylesheet">

	<!-- Vendor CSS Files -->
	<link href="~/lp/assets/vendor/aos/aos.css" rel="stylesheet">
	<link href="~/lp/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
	<link href="~/lp/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
	<link href="~/lp/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
	<link href="~/lp/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

	<!-- Template Main CSS File -->
	<link href="~/lp/assets/css/style.css" rel="stylesheet">

</head>
<body>
	<!-- ======= Header ======= -->
	<header id="header" class="d-flex align-items-center ">
		<div class="container-fluid container-xxl d-flex align-items-center">

			<div id="logo" class="me-auto">
				<a asp-area="" asp-controller="Home" asp-action="Index" class="scrollto"><img src="~/imgs/logohorizontal.png" /></a>
			</div>

			<nav id="navbar" class="navbar order-last order-lg-0">
				<ul>
					<li><a class="nav-link scrollto active" asp-area="" asp-controller="Home" asp-action="Index">Accueil</a></li>
					<li><a class="nav-link scrollto" href="#about">À propos</a></li>
					<li class="dropdown">
						<a href="#"><span>Services</span> <i class="bi bi-chevron-down"></i></a>
						<ul>
							<li class="dropdown">
								<a href="#"><span>Marché du Travail</span> <i class="bi bi-chevron-right"></i></a>
								<ul>
									<li><a href="#">CV + Lettre | LinkedIn</a></li>
								</ul>
							</li>
							<li class="dropdown">
								<a href="#"><span>Arrivé au Québec</span> <i class="bi bi-chevron-right"></i></a>
								<ul>
									<li><a href="#">Réalocation</a></li>
									<li><a href="#">Transfer</a></li>
									<li><a href="#">Suivi des demandes</a></li>
									<li><a href="#">Suivi / Journée</a></li>
								</ul>
							</li>

							<li class="dropdown">
								<a href="#"><span>Achats</span> <i class="bi bi-chevron-right"></i></a>
								<ul>
									<li><a href="#">Rédirection des achats</a></li>
									<li><a href="#">Personal Shopper</a></li>
								</ul>
							</li>
							<li class="dropdown">
								<a href="#"><span>Tourisme</span> <i class="bi bi-chevron-right"></i></a>
								<ul>
									<li><a href="#">Visite guidée</a></li>
								</ul>
							</li>
						</ul>
					</li>
					<li><a class="nav-link scrollto" href="#schedule">Nous joindre</a></li>
				</ul>
				<i class="bi bi-list mobile-nav-toggle"></i>
			</nav><!-- .navbar -->
			<a class="buy-tickets scrollto" asp-area="" asp-controller="Admin" asp-action="Index">Admin</a>

		</div>
	</header><!-- End Header -->
	<main role="main" class="pb-3">
		@RenderBody()
	</main>



	<!-- ======= Footer ======= -->
	<footer id="footer" style="background-color: #7f248d">
		<div class="container" style="background-color: #7f248d">
			&copy; 2023 - Two Canada Experience - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
		</div>
	</footer><!-- End  Footer -->

	<a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

	<!-- Vendor JS Files -->
	<script src="~/lp/assets/vendor/aos/aos.js"></script>
	<script src="~/lp/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
	<script src="~/lp/assets/vendor/glightbox/js/glightbox.min.js"></script>
	<script src="~/lp/assets/vendor/swiper/swiper-bundle.min.js"></script>
	<script src="~/lp/assets/vendor/php-email-form/validate.js"></script>

	<!-- Template Main JS File -->
	<script src="~/lp/assets/js/main.js"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
